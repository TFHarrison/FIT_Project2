{
"$schema": "https://vega.github.io/schema/vega-lite/v5.json",
"width": 800,
"height": 600,
"title": "Tide levels at WilliamsTown ",
"data": {
"url": "https://raw.githubusercontent.com/TFHarrison/WK10_HW/main/Tides.csv"
},
"mark": "line",

"params": [
{
"name":"Year",
"value":1966,
"bind":{
"input":"range",
"min":1966,
"max":2022,
"step":1,
"name":"Year : "
}
}
],
"transform": [
{"filter": "datum.Year == Year"}
],

"encoding": {
"x": {
"field": "Mth",
"type": "nominal",
"title": "Month"
},
"y": {
"field": "Mean",
"type": "quantitative",
"title": "Mean Level"
},
"color": {
"field": "Year",
"type": "nominal"
},
}

}
