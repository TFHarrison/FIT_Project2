{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "width": 800,
    "height": 600,
    "title": "Average Tide per month",
    "data": {
        "url": "https://raw.githubusercontent.com/TFHarrison/FIT_Project2/main/WK10%20hW/Tidal.csv"
    },
    "mark":"line",
    "encoding": {
        "x": {
            "field": "Mth",
            "type": "quantitative",
            "title": "Month",
            "axis": {"tickCount": 12},
            "scale": {"domain":[1,12]}
        },
        "y": {
            "field": "Mean",
            "type": "quantitative",
            "title": "Mean Level",
            "axis": {"tickCount": 10},
            "scale": {"domain":[0,1]}
        }
    }
}
