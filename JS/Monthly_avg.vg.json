{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "width": 500,
    "height": 500,
    "title": "Average Rainfall per month",
    "data": {
        "url": "https://raw.githubusercontent.com/TFHarrison/FIT_Project2/main/Clean/Monthly_avg.csv"
    },
    "mark":"line",
    "encoding": {
        "x": {
            "field": "Month",
            "type": "quantitative",
            "title": "Month",
            "axis": {"tickCount": 12},
            "scale": {"domain":[1,12]}
        },
        "y": {
            "field": "Rain",
            "type": "quantitative",
            "title": "Mean Rain (ml)",
            "axis": {"tickCount": 20},
            "scale": {"domain":[0,100]}
        }
    }
}
